<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.tohoya">
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.UPDATE_DEVICE_STATE" />

    <!-- 인터넷 관련 퍼미션 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <!-- // 인터넷 관련 퍼미션 -->

    <!-- GCM 관련 퍼미션 -->
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />
    <uses-permission android:name="com.google.android.c2dm.intent.RECEIVE" />
    <uses-permission android:name="com.tohoya.permission.C2D_MESSAGE" />
    <!--<uses-permission android:name="com.tohoya.permission.C2D_MESSAGE" android:protectionLevel="signature"/>-->
    <!-- // GCM 관련 퍼미션 -->

    <application android:allowBackup="true" android:label="@string/app_name"
        android:supportsRtl="true">
        <!--com.google.android.gms.gcm.GcmReceiver"-->
        <receiver
            android:name="com.tohoya.gcm.GcmBroadcastReceiver"
            android:exported="true"
            android:permission="com.google.android.c2dm.permission.SEND" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <category android:name="com.tohoya.gcm" />
            </intent-filter>
        </receiver>
        <service
            android:name="com.tohoya.gcm.JwListenerService"
            android:exported="false" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
            </intent-filter>
        </service>
        <service
            android:name="com.tohoya.gcm.JwIDListenerService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.android.gms.iid.InstanceID" />
            </intent-filter>
        </service>
        <service
            android:name="com.tohoya.gcm.RegistrationIntentService"
            android:exported="false">
        </service>
        <!-- [START firebase_service] -->
        <service
            android:name="com.tohoya.fcm.JwFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>
        <!-- [END firebase_service] -->
        <!-- [START firebase_iid_service] -->
        <service
            android:name="com.tohoya.fcm.JwFirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT"/>
            </intent-filter>
        </service>
        <!-- [END firebase_iid_service] -->
    </application>
    <!--<receiver-->
        <!--android:name="com.tohoya.gcm.GcmBroadcastReceiver"-->
        <!--android:permission="com.google.android.c2dm.permission.SEND" >-->
        <!--<intent-filter>-->
            <!--<action android:name="com.google.android.c2dm.intent.RECEIVE" />-->
            <!--<category android:name="com.tohoya" />-->
        <!--</intent-filter>-->
    <!--</receiver>-->


</manifest>
